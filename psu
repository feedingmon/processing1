void setup(){
  size(800, 300);
  textSize(100);
}
int i, dir=1;
void draw(){
  fill(0);
  background(255,255,0);
  text("Graphics", i, 200);
  if(i>width) dir=-1;
  if(i<0) dir=1;
  i = i+dir;
}

